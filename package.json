{
  "name": "lexipro-forensic-os",
  "version": "1.2.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev:client": "vite",
    "dev:server": "cd server && npm run dev",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "build:embed": "vite build -c vite.config.embed.ts",
    "preview": "vite preview",
    "test": "npm --prefix server test",
    "seed": "npm --prefix server run db:seed",
    "demo:reset": "node scripts/demo-reset.mjs",
    "demo:up": "node scripts/demo-up.mjs",
    "demo:perfect": "node scripts/demo-perfect.mjs",
    "demo:gold": "node scripts/demo-gold.mjs",
    "verify:packet": "node scripts/verify-proof-packet.mjs",
    "demo:smoke": "playwright test e2e/demo-smoke.spec.ts",
    "ready:demo": "npm run build && npm --prefix server run build && npm --prefix server test",
    "demo:reset:win": "powershell -NoProfile -Command \"$env:OLLAMA_MODEL='gemma:2b'; $env:OLLAMA_EMBED_MODEL='nomic-embed-text'; $env:STORAGE_ENCRYPTION_REQUIRED='false'; $env:DATABASE_URL='postgresql://postgres:postgres@localhost:5433/lexipro'; docker exec -i lexipro-db psql -U postgres -d lexipro -c \\\"DROP SCHEMA public CASCADE; CREATE SCHEMA public; CREATE EXTENSION IF NOT EXISTS vector;\\\"; cd server; npx prisma db push --accept-data-loss --schema prisma/schema.prisma; npx tsx scripts/seed_golden_demo.ts; npx tsx scripts/verify-demo-readiness.ts\"",
    "demo:reset:unix": "bash scripts/demo-reset.sh",
    "smoke:ci": "node -e \"console.log('SMOKE OK')\"",
    "qa:full": "node tools/qa-browser-worker/run-qa.mjs",
    "qa:agent": "node tools/qa-browser-worker/agent-console/agent.mjs",
    "qa:drive": "node tools/qa-browser-worker/agent-console/run-sequence.mjs",
    "qa:autonomous": "node tools/qa-browser-worker/agent-console/autonomous.mjs",
    "qa:diagnose": "node tools/qa-browser-worker/diagnose-ui.mjs",
    "codex:bridge": "node tools/codex-bridge/server.mjs",
    "ps:bridge": "node tools/powershell-bridge/server.mjs",
    "lint": "eslint . --max-warnings 0",
    "test:e2e": "playwright test",
    "proof:ai": "npm --prefix server run proof:ai",
    "proof:packet": "npm --prefix server run proof:packet",
    "diligence:pack": "node scripts/generate-diligence-pack.mjs",
    "audit": "npm audit --omit=dev && npm --prefix server run audit",
    "docker:up": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "security:zap": "bash scripts/zap-scan.sh"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@google/generative-ai": "^0.21.0",
    "@tiptap/extension-highlight": "^3.16.0",
    "@tiptap/react": "^3.16.0",
    "@tiptap/starter-kit": "^3.16.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "driver.js": "^1.4.0",
    "framer-motion": "^11.11.11",
    "jszip": "^3.10.1",
    "lucide-react": "^0.460.0",
    "pdfjs-dist": "5.4.296",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-pdf": "^10.3.0",
    "react-resizable-panels": "^4.5.9",
    "react-router-dom": "^7.12.0",
    "tailwind-merge": "^2.5.4",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/containers": "^0.1.0",
    "@playwright/test": "^1.50.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "vite": "^6.4.1"
  }
}
