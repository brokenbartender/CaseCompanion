{
    "name": "lexipro-forensic-backend",
    "version": "1.2.0",
    "description": "Secure forensic proxy and data management server",
    "main": "dist/index.js",
    "type": "module",
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "dev": "tsx watch index.ts",
        "build": "tsc --noEmit",
        "typecheck": "tsc --noEmit",
        "lint": "node ../node_modules/.bin/eslint . --max-warnings 0",
        "start": "tsx index.ts",
        "start:ts": "tsx index.ts",
        "pretest": "node ./scripts/prisma-generate.js",
        "test": "node ./scripts/run-tests.js",
        "test:fast": "node ./scripts/run-tests-fast.js",
        "proof:ai": "tsx scripts/proof-ai.ts",
        "proof:packet": "tsx scripts/proof-packet.ts",
        "proof:packet:check": "tsx scripts/proof-packet-check.ts",
        "proof:packet:verify": "tsx scripts/proof-packet-verify.ts",
        "shredder:report": "tsx scripts/generate-key-shredder-report.ts",
        "audit": "npm audit --omit=dev",
        "security-check": "npm audit --audit-level=high",
        "db:generate": "prisma generate",
        "db:exec": "tsx scripts/db-exec.ts",
        "db:migrate": "prisma migrate dev --name init",
        "db:deploy": "prisma migrate deploy",
        "db:push": "npm run db:exec && prisma db push --accept-data-loss",
        "db:seed": "prisma db seed",
        "db:studio": "prisma studio",
        "sabotage": "ts-node scripts/sabotage_audit.ts",
        "db:wait": "tsx scripts/wait-for-db.ts",
        "ingest:text": "tsx scripts/ingest_text_dir.ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.712.0",
        "@google/genai": "^1.34.0",
        "@google/generative-ai": "^0.21.0",
        "@prisma/client": "6.0.0",
        "@types/adm-zip": "^0.5.7",
        "adm-zip": "^0.5.16",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-rate-limit": "^8.2.1",
        "ipaddr.js": "^2.1.0",
        "jsonwebtoken": "^9.0.2",
        "lexipro-forensic-os": "file:..",
        "multer": "^2.0.2",
        "node-cron": "^3.0.3",
        "openid-client": "^6.8.1",
        "pdf-lib": "^1.17.1",
        "pdfjs-dist": "^4.8.69",
        "playwright": "^1.57.0",
        "puppeteer": "^24.36.0",
        "redis": "^4.6.13",
        "xlsx": "^0.18.5",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/multer": "^1.4.13",
        "@types/node": "^22.10.2",
        "@types/node-cron": "^3.0.11",
        "@types/supertest": "^6.0.3",
        "nodemon": "^3.1.9",
        "prisma": "6.0.0",
        "supertest": "^7.2.2",
        "ts-node": "^10.9.2",
        "tsx": "^4.21.0",
        "typescript": "^5.7.2"
    },
    "prisma": {
        "schema": "prisma/schema.prisma",
        "seed": "tsx prisma/seed.ts"
    }
}
